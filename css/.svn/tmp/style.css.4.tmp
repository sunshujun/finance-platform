		    .modal .add-text1{display:inline-block;padding-right:15px;text-align: right;width:85px}
		   /*接入节点控制*/
		    #node-check .modal-body, #pass-check .modal-body{padding:8px 15px;}
		    .modal .modal-body span{color:#FF0000}
		    #node-check .modal-dialog{width:500px}
		    #pass-check .modal-dialog{width:400px}
		    #node-check .form-control{display:inline-block;width:300px}
		    #pass-check .form-control{display:inline-block;width:250px}
		    
		    /*渠道管理 资金端服务接出配置modal */
		    #fund-service .modal-body{font-size:13px;padding:8px 10px 15px 10px;}
		    #fund-service .modal-body div{margin-bottom:0;}
		    #fund-service .modal-dialog{width:55%;min-width:600px;}
            #fund-service .fund-service-head{background:#FDFDFD;font-weight:bold;border:1px solid #DDDDDD;border-radius: 5px 5px 0 0;}
            #fund-service .fund-service-head div{border:none}
            #fund-service .fund-service-wrap{border:1px solid #DDDDDD;border-radius: 0 0 5px 5px;border-top: none;}
            #fund-service .fund-service-wrap input{width:13px;height:13px;margin-left:8px;}
            #fund-service .fund-service-wrap button{font-size:13px;color:#FFFFFF}
            #fund-service .fund-service-wrap span{color:#FFFFFF}
            #fund-service .fund-service-wrap div,#fund-service .fund-service-head div{display:inline-block;height:30px; line-height: 30px;}
            #fund-service .fund-service-outer{width:100%;border-bottom:1px solid #DDDDDD;}
            #fund-service .modal-body div{text-align:left;}
            #fund-service .div1{width:130px;padding-left:15px;border-right:1px solid #DDDDDD;}
            #fund-service .div2{width:65%;}
            #fund-service .div3{width:62px;float:right;border-left:1px solid #DDDDDD;padding-left:4px;}
            #fund-service .fund-service-outer:nth-child(odd){background:#F5F5F5}
            #fund-service .fund-service-outer:nth-child(even){background:#D5ECBF}
           
            #channel-service .header{min-width:260px}
            #channel-service .row>div{padding-left:0}
            
            
            
